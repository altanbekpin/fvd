    <!-- http://www.semanticweb.org/kazontolgy#Adv -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#Adv"><kazontolgy:question xml:lang="kz">Қалай?</kazontolgy:question>
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/kazontolgy#NAdv"/>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/kazontolgy#root"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.semanticweb.org/kazontolgy#hasPOS"/>
                                <owl:allValuesFrom rdf:resource="http://www.semanticweb.org/kazontolgy#Adv"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#POS"/>
        <kazontolgy:definition xml:lang="kg">&quot;Кыймыл-аракеттин ар түрдүү кырдаалдарын (орундук,
 мезгилдик, себеп, максат, сан-өлчөм, сын-сыпат
 кырдаалдарын) көрсөтүп, сүйлөмдө көбүнчө этиш сөздөр
 менен айкашып келип бышыктоочтук милдет аткарган
 сөздөр тактоочтор деп аталат.&quot;</kazontolgy:definition>
        <kazontolgy:definition xml:lang="tr">Bir önadın, bir eylemin ya da bir başka belirtecin önüne gelerek onun anlamını nitelik, nicelik, durum ve zaman bakımından tanımlayan sözcüğe belirteç denir.</kazontolgy:definition>
        <kazontolgy:definition xml:lang="kz">қимылдың, іс-әрекеттің әртүрлі белгісін: амалын, тәсілін, мекенін, мезгілін, себебін, мақсатын, т.б. сипаттарын білдіретін сөздер</kazontolgy:definition>
        <rdfs:label xml:lang="tr">Belirteç</rdfs:label>
        <rdfs:label xml:lang="kg">Тактооч</rdfs:label>
        <rdfs:label xml:lang="kz">Үстеу</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#AdvCaus -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#AdvCaus">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#Adv"/>
        <kazontolgy:definition xml:lang="kz">қимылдың, іс-әрекеттің болу себебін білдіреді</kazontolgy:definition>
        <kazontolgy:question xml:lang="kz">не себепті? қалай? неге?</kazontolgy:question>
        <rdfs:label xml:lang="kz">Себеп-салдар үстеуі</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#AdvDMQ -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#AdvDMQ">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#Adv"/>
        <kazontolgy:definition xml:lang="tr">Eylemlerin, sıfatların, belirteçlerin anlamlarını azlık, çokluk bakımından etkileyen sözcüklerdir</kazontolgy:definition>
        <kazontolgy:definition xml:lang="kg">кыймыл-аракеттин көлөмдүк, өлчөмдүк белгилерин көрсөтөт.</kazontolgy:definition>
        <kazontolgy:definition xml:lang="kz">сынның немесе қимылдың көлемдік дәрежесін, мөлшерін, шама-шарқын білдіреді</kazontolgy:definition>
        <kazontolgy:question xml:lang="kz">қанша? қаншама? қаншалық? қаншалап?</kazontolgy:question>
        <rdfs:isDefinedBy xml:lang="tr">Ölçü belirteçleri</rdfs:isDefinedBy>
        <rdfs:label xml:lang="kz">Мөлшер үстеуі</rdfs:label>
        <rdfs:label xml:lang="kg">Сан-өлчөм аныктагыч тактоочтор</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#AdvGrp -->

