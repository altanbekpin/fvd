    <!-- http://www.semanticweb.org/kazontolgy#Prost -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#Prost">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#SEMANTIC"/>
        <rdfs:comment xml:lang="kz">Кеңістіктік</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#PrstDef -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#PrstDef">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#PrstTense"/>
        <kazontolgy:definition xml:lang="kz">іс- әрекет пне қимылдың сөйлеп тұрған нақ сәтінде болуын білдіреді</kazontolgy:definition>
        <rdfs:label xml:lang="kz">нақ осы шақ</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#PrstGoal -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#PrstGoal">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#PrstTense"/>
        <rdfs:label xml:lang="kz">Мақсатты осы шақ</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#PrstTense -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#PrstTense">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#TENSE_category"/>
        <kazontolgy:definition xml:lang="tatar">&quot;сөйләү моменты белән бер үк вакытта башкарылган,
сөйләү моменты өчен актуаль булган эш-хәлләрне белдерә&quot;</kazontolgy:definition>
        <kazontolgy:definition xml:lang="tr">Eylemin belirttiği iş ve oluşun içinde bulunduğumuz zamanda gerçekleştiğini bildirir. Şimdiki zaman eki -ıyor, -iyor, -uyor, -üyor</kazontolgy:definition>
        <kazontolgy:definition xml:lang="kg">Кыймыл-аракеттин сүйлөп жаткан учурда болуп жаткандыгын көрсөтүүчү этиш сөздөр учур чак деп аталат.</kazontolgy:definition>
        <kazontolgy:definition xml:lang="kz">іс-әрекет пен қимылдың сөйлеп тұрған сәтте орындалып жатқанын білдіреді</kazontolgy:definition>
        <rdfs:label xml:lang="tr">Şimdiki zaman</rdfs:label>
        <rdfs:label xml:lang="kz">Осы шақ</rdfs:label>
        <rdfs:label xml:lang="kg">Учур чак</rdfs:label>
        <rdfs:label xml:lang="tatar">Хәзерге заман</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#PrstTrans -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#PrstTrans">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#PrstTense"/>
        <kazontolgy:definition xml:lang="kz">іс-әрекеттің сөйлеп тұрған кезде болатынын әрі ұласпалылық сипатын танытады</kazontolgy:definition>
        <rdfs:label xml:lang="kz">ауыспалы осы шақ</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#Prtcl -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#Prtcl">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#FW"/>
        <kazontolgy:definition xml:lang="tatar">Аерым сүз яки җөмләгә мәгънә төсмере өстәп килә торган модаль сүз төркеме</kazontolgy:definition>
        <kazontolgy:definition xml:lang="kz">өздері тіркесетін сөздерге әр қилы қосымша реңктер (мағыналар) жамайтын сөздер. Демеуліктер өзгермейді, түрленбейді, тіркескен сөздеріне қосымша мағына үстейді.</kazontolgy:definition>
        <rdfs:label xml:lang="kz">Демеулік</rdfs:label>
        <rdfs:label xml:lang="tatar">Кисәкчә</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#PsBsAnTlBn -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#PsBsAnTlBn">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#5SP"/>
        <rdfs:label xml:lang="kz">PsBsAnTlBn</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#PsBsTlBn -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#PsBsTlBn">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#4SP"/>
        <rdfs:label xml:lang="kz">PsBsTlBn</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#PsTlAnBsBn -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#PsTlAnBsBn">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#5SP"/>
        <rdfs:label xml:lang="kz">Пысықтауыш,Толықтауыш,Анықтауыш,Бастауыш,Баяндауыш тан тұратын бес мүшелі сөйлем</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#Pstp -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#Pstp">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#FW"/>
        <kazontolgy:definition xml:lang="kz">объекті мен объектінің не предикаттың арасындағы түрлі грамматикалық қатынастарды білдіру үшін қолданылып, белгілі бір септік жалғауын меңгеріп тұратын көмекші сөздер</kazontolgy:definition>
        <kazontolgy:definition xml:lang="tatar">Җөмләдә предлогка охшаш морфологик-синтаксик функция башкарган, әмма ияргән сүзеннән соң килеп, сүзләр арасында бергәлек, чагыштыру, сәбәп һ.б.ш. мөнәсәбәтләр оештыруда катнаша торган ярдәмче сүз төркеме</kazontolgy:definition>
        <rdfs:label xml:lang="tatar">Бәйлек</rdfs:label>
        <rdfs:label xml:lang="kz">Септеулік</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#PstpAbl -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#PstpAbl">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#Pstp"/>
        <kazontolgy:definition xml:lang="kz">шығыс септік жалғауларында тұрған сөзбен тіркесіп қолданылады.</kazontolgy:definition>
        <rdfs:label xml:lang="tatar">Чыгыш килешен таләп итүче</rdfs:label>
        <rdfs:label xml:lang="kz">Шығыс септікті меңгеретін септеуліктер</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#PstpDir -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#PstpDir">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#Pstp"/>
        <kazontolgy:definition xml:lang="kz">барыс септік жалғауларында тұрған сөзбен тіркесіп қолданылады.</kazontolgy:definition>
        <rdfs:label xml:lang="kz">Барыс септікті меңгеретін септеуліктер</rdfs:label>
        <rdfs:label xml:lang="tatar">Юнәлеш килешен таләп итүче</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#PstpInst -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#PstpInst">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#Pstp"/>
        <kazontolgy:definition xml:lang="kz">көмектес септік жалғауларында тұрған сөзбен тіркесіп қолданылады.</kazontolgy:definition>
        <rdfs:label xml:lang="kz">Көмектес септікті меңгеретін септеуліктер</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#PstpNom -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#PstpNom">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#Pstp"/>
        <kazontolgy:definition xml:lang="tr">Bu ilgeçlerden önce gelen sözcükler yalın durumda olur</kazontolgy:definition>
        <kazontolgy:definition xml:lang="kz">атау септік жалғауларында тұрған сөзбен тіркесіп қолданылады.</kazontolgy:definition>
        <rdfs:label xml:lang="tr">Yalın Durum İsteyen İlgeçler</rdfs:label>
        <rdfs:label xml:lang="kz">Атау септікті меңгеретін септеуліктер</rdfs:label>
        <rdfs:label xml:lang="tatar">Баш килешне таләп итүче</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#Pstv -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#Pstv">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#Emtn"/>
        <kazontolgy:definition xml:lang="tr">Sevinç, heyecan, şaşkınlık gibi duygularını belirten ünlemlerdir. a / aa, ah, vah,aman,ay, ayol, bravo, e, eee, eh, hay, ha, hah, eyvah, hadi / haydi, heyhat, keşki, oh, of, öf, püf, tu /tü, ya, ulan, tüh, yuh / yuha, yazık, vallahi:</kazontolgy:definition>
        <kazontolgy:definition xml:lang="kz">жағымды мағынаға ие одағайлар</kazontolgy:definition>
        <rdfs:label xml:lang="tr">Pozitiv Duygu ünlemi</rdfs:label>
        <rdfs:label xml:lang="kz">Жағымды көңіл күйді білдіретін</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#Qual -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#Qual">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#Adj"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#Quan -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#Quan">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/kazontolgy#DrvtvNum"/>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/kazontolgy#root"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.semanticweb.org/kazontolgy#hasPOS"/>
                                <owl:allValuesFrom rdf:resource="http://www.semanticweb.org/kazontolgy#Num"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#Num"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#Recp -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#Recp">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#VOICE_category"/>
        <kazontolgy:definition xml:lang="kg">&quot;субъектилердин өз ара, же биргелешип
 иштешкен кыймыл-аракетин билдирет.&quot;</kazontolgy:definition>
        <kazontolgy:definition xml:lang="tatar">«ике яки берничә затның уртак эшен» белдерүче</kazontolgy:definition>
        <kazontolgy:definition xml:lang="tr">İşteş fiilerde eylem birden fazla özne tarafından birlikte ya da karşılıklı yapılır. Fiili işteş yapabilmek için Türkçede -ş, -ış, -iş, -uş, -üş , eki kullanılır.</kazontolgy:definition>
        <kazontolgy:definition xml:lang="kz">қимыл, іс-әрекеттің иесі, орындаушысы біреу емес, бірнешеу болатын етіс</kazontolgy:definition>
        <rdfs:label xml:lang="tr">İşteş Çatı</rdfs:label>
        <rdfs:label xml:lang="kg">Кош мамиле</rdfs:label>
        <rdfs:label xml:lang="kz">Ортақ етіс</rdfs:label>
        <rdfs:label xml:lang="tatar">Уртаклык юнәлеше</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#Refl -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#Refl">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#Pron"/>
        <kazontolgy:definition xml:lang="tr">Kişi kavramını pekiştirerek belirten ya da yapılan işin yapana dönüşünü gösteren sözcüklere dönüşiü adıl denir.</kazontolgy:definition>
        <kazontolgy:definition xml:lang="kz">әрқандай тұлғадағы өз деген сөз</kazontolgy:definition>
        <rdfs:label xml:lang="tr">Dönüşlü adıllar</rdfs:label>
        <rdfs:label xml:lang="kz">Өздік есімдігі</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#Reflv -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#Reflv">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#VOICE_category"/>
        <kazontolgy:definition xml:lang="kg">&quot;Өздүк мамиле-кыймыл-аракеттин реалдуу субъектиге
 багытталганын, же анын айланасында топтолгонун көрсөтөт.&quot;</kazontolgy:definition>
        <kazontolgy:definition xml:lang="tr">Dönüşlü fiillerde eylemi yapan özne aynı zamanda ondan etkilenir. Nesne yoktur. Bu fiiller -n, -ın, -in, -un, -ün , -l, ıl, -il, -ul, -ül ,eki alırlar ve geçişsizdirler</kazontolgy:definition>
        <kazontolgy:definition xml:lang="tatar">«эш башкаручының үзенә әйләнеп кайткан эш»ен белдерүче юнәлеш</kazontolgy:definition>
        <kazontolgy:definition xml:lang="kz">қимылдың іс-әрекеттің орындаушысына, іс иесіне тікелей қатыстылығын білдіреді</kazontolgy:definition>
        <rdfs:label xml:lang="tr">Dönüşlü Çatı</rdfs:label>
        <rdfs:label xml:lang="tatar">Кайтым юнәлеше</rdfs:label>
        <rdfs:label xml:lang="kz">Өздік етіс</rdfs:label>
        <rdfs:label xml:lang="kg">Өздүк мамиле</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#Rel -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#Rel">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#Adj"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#Rstrc -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#Rstrc">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#Prtcl"/>
        <kazontolgy:definition xml:lang="kz">білгілі бір нәрсеге, іс-әрекетке, амалға,сын-сипатқа, мезгілге т.б. шек қоя тұжырымдап айту үшін қолданылатын демеуліктер</kazontolgy:definition>
        <rdfs:label xml:lang="tatar">Чикләү, аерып кую мәгънәсен белдерүче</rdfs:label>
        <rdfs:label xml:lang="kz">Шектеу</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#S -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#S">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#SYNTAX"/>
        <kazontolgy:definition xml:lang="kz">Сөйлем дегеніміз – сөздердің  әр алуан тұлғада өзара тіркесіп, біршама тиянақталған ойды білдіру</kazontolgy:definition>
        <rdfs:label xml:lang="tr">Cümle</rdfs:label>
        <rdfs:label xml:lang="kg">Сүйлөм</rdfs:label>
        <rdfs:label xml:lang="kz">Сөйлем</rdfs:label>
        <rdfs:label xml:lang="tatar">Җөмлә</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#SEMANTIC -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#SEMANTIC">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#GRAMMAR"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#SFCA -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#SFCA">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#Impr"/>
        <kazontolgy:definition xml:lang="tr">Tabiattaki canlı, cansız varlıkların seslerini taklit eden ünlemlerdir.</kazontolgy:definition>
        <kazontolgy:definition xml:lang="kz">үй хайуандарын шақыру, айдау, үркіту, қорғау мақсатымен қолданылатын сөздер мен одағайлы тіркестер жатады</kazontolgy:definition>
        <rdfs:label xml:lang="tr">Yansıma ünlemleri</rdfs:label>
        <rdfs:label xml:lang="kg">Айбандарды чакыруу, айдоо үчүн айтылган сырдык сөздөр</rdfs:label>
        <rdfs:label xml:lang="kz">Малға, үй хайуандарына бағышталып айтылатын одағайлар</rdfs:label>
        <rdfs:label xml:lang="tatar">хайваннарны, кош-кортларны чакыру һәм куу сүзләре</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#SFCP -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#SFCP">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#Impr"/>
        <kazontolgy:definition xml:lang="tr">Bir isim veya isim unsurunun önüne gelerek, bunların işaret ettiği şahıs, varlık veya
kavrama yönelik seslenmeyi kuvvetlendiren ünlemlerdir: ey, hey, yâ, yâhu, be, a, bre. Yazı
dilinde “ey” ve “hey” kullanılır:</kazontolgy:definition>
        <kazontolgy:definition xml:lang="kz">Бұған адамның адресіне бұйыру, жекіру, тыйым салу мақсатымен қолданылады</kazontolgy:definition>
        <rdfs:label xml:lang="tr">Seslenme Ünlemleri</rdfs:label>
        <rdfs:label xml:lang="kz">Адамға бағышталып айтылатын одағайлар</rdfs:label>
        <rdfs:label xml:lang="tatar">Эндәшү, мөрәҗәгать итүне белдерә торган ымлыклар</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#SG -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#SG">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#Number"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#SGPP -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#SGPP">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/kazontolgy#hasDependent"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/kazontolgy#Pers"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/kazontolgy#hasHead"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/kazontolgy#P1SG1_C"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#AGRM"/>
        <rdfs:label xml:lang="kz">Жекеше түрдегі жіктік жалғауымен жасалатын қиысу</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#SG_C -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#SG_C">
