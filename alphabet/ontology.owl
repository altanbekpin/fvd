        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/kazontolgy#hasAffix"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/kazontolgy#ABL"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/kazontolgy#hasRoot"/>
