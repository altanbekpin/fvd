                        <owl:onProperty rdf:resource="http://www.semanticweb.org/kazontolgy#hasAffix"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/kazontolgy#LOC"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/kazontolgy#hasRoot"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://www.semanticweb.org/kazontolgy#root"/>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://www.semanticweb.org/kazontolgy#hasPOS"/>
                                        <owl:allValuesFrom rdf:resource="http://www.semanticweb.org/kazontolgy#Pron"/>
                                    </owl:Restriction>
                                </owl:intersectionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#LOC_C"/>
        <rdfs:label xml:lang="kz">Жатыс септіктегі есімдік</rdfs:label>
    </owl:Class>
    


