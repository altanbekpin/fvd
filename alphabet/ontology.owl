    <!-- http://www.semanticweb.org/kazontolgy#ACC -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#ACC">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#Cases"/>
        <kazontolgy:instance xml:lang="kz">-ды, -ді, -ты, -ті, -ны, -ні</kazontolgy:instance>
        <rdfs:label xml:lang="kz">Табыс септік жалғаулары</rdfs:label>
        <rdfs:label xml:lang="tatar">Төшем килеше кушымчалары</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#ACC_C -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#ACC_C">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#CASES_category"/>
        <kazontolgy:definition xml:lang="tr">Adın eylemden doğrudan doğruya etkilenme ve ilgi durumudur.</kazontolgy:definition>
        <kazontolgy:definition xml:lang="kz">Табыс септік тура объектіні білдіреді де, сабақты етістіктермен тіркесіп, сөйлемде тура толықтауыш қызметін атқарады.</kazontolgy:definition>
        <kazontolgy:question xml:lang="tr">kimi?  Neyi</kazontolgy:question>
        <kazontolgy:question xml:lang="kg">Кимди? Эмнени?</kazontolgy:question>
        <kazontolgy:question xml:lang="tatar">кемне? нәрсәне?</kazontolgy:question>
        <kazontolgy:question xml:lang="uz">кимни? болани?</kazontolgy:question>
        <kazontolgy:question xml:lang="kz">кімді? нені?</kazontolgy:question>
        <rdfs:label xml:lang="tr">Belirtme hali</rdfs:label>
        <rdfs:label xml:lang="kz">Табыс септік</rdfs:label>
        <rdfs:label xml:lang="kg">табыш</rdfs:label>
        <rdfs:label xml:lang="uz">тушум</rdfs:label>
        <rdfs:label xml:lang="tatar">төшем килеше</rdfs:label>
    </owl:Class>
    


