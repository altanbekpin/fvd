        <rdfs:label xml:lang="tr">Tekil</rdfs:label>
        <rdfs:label xml:lang="kg">Жекелик</rdfs:label>
        <rdfs:label xml:lang="kz">Жекеше</rdfs:label>
        <rdfs:label xml:lang="tatar">берлек сан</rdfs:label>
        <rdfs:label xml:lang="uz">бирлик</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#SIM3 -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#SIM3">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#NWC"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#SP -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#SP">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#SYNTAX"/>
        <rdfs:label xml:lang="kz">Сөйлем мүшелерi</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#SS -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#SS">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#S"/>
        <kazontolgy:definition xml:lang="kz">Бір немесе бірнеше сөз бен сөз тіркестерінен құралып, тиянақты бір ғана ойды білдіретін сөйлем.</kazontolgy:definition>
        <kazontolgy:example xml:lang="kz">Әйгерім мектепке  апаратын кітаптарын дайындады</kazontolgy:example>
        <rdfs:label xml:lang="kz">жай сөйлем</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#SSP -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#SSP">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#SP"/>
        <rdfs:label xml:lang="kz">Тұрлаусыз сөйлем мүшелері</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#SYNTAX -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#SYNTAX">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#GRAMMAR"/>
        <kazontolgy:definition xml:lang="kz">Сөз тіркесі мен сөйлемнің құрамы мен құрылымын зерттейтін ғылым.</kazontolgy:definition>
        <rdfs:label xml:lang="kz">Синтаксис</rdfs:label>
        <rdfs:label xml:lang="tatar">Синтаксис</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#Sabakty -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#Sabakty">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#CCS"/>
        <kazontolgy:definition xml:lang="kz">Сабақтас құрмалас сөйлем деп құрамындағы жай сөйлемдердің бірі екіншісіне бағына байланысқан сөйлемді айтамыз</kazontolgy:definition>
        <rdfs:label xml:lang="kz">сабақтас құрмалас сөйлем</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#Salalas -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#Salalas">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#CCS"/>
        <kazontolgy:definition xml:lang="kz">Құрамындағы жай сөйлемдердің баяндауыштары тиянақты болып, өзара тең дәрежеде байланысқан құрмалас сөйлем салалас құрмалас сөйлем деп аталады</kazontolgy:definition>
        <rdfs:label xml:lang="kz">салалас құрмалас сөйлем</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#Sbrd -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#Sbrd">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#Conj"/>
        <kazontolgy:definition xml:lang="kg">&quot;Кошмок сүйлөмдүн тутумундагы сүйлөмдөрдү бири-
 бирине кандайдыр деңгээлде көз каранды абалда тутуп
 байланыштырган байламталар багындыргыч байламталар
 деп аталат</kazontolgy:definition>
        <kazontolgy:definition xml:lang="tatar">Гади җөмләдә иярүче сүзне ияртүче сүзгә буйсындыра, кушма җөмләдә иярчен җөмләне баш җөмләгә ияртә торган теркәгечләр</kazontolgy:definition>
        <kazontolgy:definition xml:lang="kz">тек құрмалас сөйлемдердің құрамдарындағы жай сөйлемдерді бір-бірімен жалғастырып байланыстырып, солардың арақатысын анықтап көрсететін жалғаулықтар</kazontolgy:definition>
        <rdfs:label xml:lang="kg">Багындыргыч байламталар</rdfs:label>
        <rdfs:label xml:lang="tatar">Ияртүче теркәгечләр</rdfs:label>
        <rdfs:label xml:lang="kz">Сабақтандырғыш</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#Sentence_structure -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#Sentence_structure">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#S"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#Space -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#Space">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/kazontolgy#isSpace"/>
                <owl:allValuesFrom rdf:resource="http://www.semanticweb.org/kazontolgy#Space"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#Prost"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#Spcf -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#Spcf">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#Prtcl"/>
        <kazontolgy:definition xml:lang="kz">жеке сөздер мен сөз тіркестерінің мағыналарын, сөйлемде айтылатын ойды нақтыландыра, тұжырымдай түсу үшін қолданылатын демеуліктер</kazontolgy:definition>
        <rdfs:label xml:lang="kz">Нақтылық</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#Sub -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#Sub">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#PSP"/>
        <rdfs:label xml:lang="kz">баяндауыш</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#SuperDeg -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#SuperDeg">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#DegComp"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#TENSE_category -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#TENSE_category">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#V_Category"/>
        <kazontolgy:definition xml:lang="tr">Eylemin hangi zamanda yapıldığını veya yapılacağını gösteren anlama eylemlerde zaman denir.</kazontolgy:definition>
        <kazontolgy:definition xml:lang="kg">Кыймыл-аракеттин сүйлөп жаткан учурда болуп жаткандыгын, же андан мурун болуп өткөндүгүн, же андан кийин болорун чындык катарында көрсөткөн этиш сөздөр этиштин чактары деп аталат.</kazontolgy:definition>
        <kazontolgy:definition xml:lang="tatar">Эш яки хәлнең сөйләм моментына үтәлү вакытын мөнәсәбәттә белдерә торган фигыльләргә хас грамматик категория (хәзерге, киләчәк, үткән заман)</kazontolgy:definition>
        <kazontolgy:definition xml:lang="kz">қимыл, амал-әрекеттің жасалу мезгілін</kazontolgy:definition>
        <rdfs:label xml:lang="tr">Zaman</rdfs:label>
        <rdfs:label xml:lang="tatar">Заман</rdfs:label>
        <rdfs:label xml:lang="kg">Чак категориясы</rdfs:label>
        <rdfs:label xml:lang="kz">Шақтар</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#TlAnBsBn -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#TlAnBsBn">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#4SP"/>
        <rdfs:label xml:lang="kz">TlAnBsBn</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#TlBsPsBn -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#TlBsPsBn">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#4SP"/>
        <rdfs:label xml:lang="kz">TlBsPsBn</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#TlPsBsBn -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#TlPsBsBn">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#4SP"/>
        <rdfs:label xml:lang="kz">TlPsBsBn</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#Trns -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#Trns">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#V"/>
        <kazontolgy:definition xml:lang="tr">Geçişli fiiller belirtili ya da belirtisiz nesne alabilen fiillerdir. Bir fiilin geçişli olup olmadığı özneden sonra sorulan &quot;ne&quot;[Not 1], &quot;neyi&quot; ve &quot;kimi&quot; soruları sorularak anlaşılır</kazontolgy:definition>
        <kazontolgy:definition xml:lang="tatar">Күчемле фигыльләрдә эш-хәл һәрвакыт туры объектка юнәлгән була</kazontolgy:definition>
        <kazontolgy:definition xml:lang="kz">етістіктің қимылы объектіге ауысып, ол объектінің табыс септікте тұру қажет</kazontolgy:definition>
        <rdfs:label xml:lang="tr">Geçişli eylem</rdfs:label>
        <rdfs:label xml:lang="tatar">Күчемле фигыль</rdfs:label>
        <rdfs:label xml:lang="kz">Сабақты етістік</rdfs:label>
        <rdfs:label xml:lang="kg">Өтмө</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#Tusindirmeli -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#Tusindirmeli">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#Salalas"/>
        <kazontolgy:definition xml:lang="kz">Құрамындағы жай сөйлемдердің алдыңғысы соңғысын нұсқап, ал соңғысы алдыңғы сөйлемнің мағынасын ашып, түсіндіріп тұратын құрмаластың түрі түсіндірмелі салалас деп аталады. Түсіндірмелі салаластың құрамындағы жай сөйлемдер жалғаулық шылаусыз, іргелес тұрып байланысады да, арасына қос нүкте қойылады.</kazontolgy:definition>
        <kazontolgy:example xml:lang="kz">Ернеудің бір айналма тұсында із екі айырылды: бірі ернеуді бойлап ілгері кетті, екіншісі төмен қарай түскен.</kazontolgy:example>
        <rdfs:label xml:lang="kz">Түсіндірмелі салалас құрмалас сөйлем</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#V -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#V">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/kazontolgy#AdvbP"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/kazontolgy#PastDef"/>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/kazontolgy#root"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.semanticweb.org/kazontolgy#hasPOS"/>
                                <owl:allValuesFrom rdf:resource="http://www.semanticweb.org/kazontolgy#V"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#POS"/>
        <kazontolgy:definition xml:lang="tr">Bir iş, oluş, hareket, durum bildiren, kişi ve zaman kavramı veren sözcüklere eylem veya  Fiil denir.</kazontolgy:definition>
        <kazontolgy:definition xml:lang="kg">Заттын кыймыл-аракетин, ал-абалын билдирүүчү сөз түркүмү этиш деп аталат.</kazontolgy:definition>
        <kazontolgy:definition xml:lang="kz">заттың қимылын, амалын, жай-күйін білдіретін сөздер. СөйлемнІң баяндауыш, пысықтауыш қызметін атқарады.</kazontolgy:definition>
        <kazontolgy:definition xml:lang="tatar">предметның эшен, хәлен процесс рәвешендә белдерә торган һәм барлык-юклык, юнәлеш, дәрәҗә, наклонение, заман, зат-сан белән төрләнә торган сүз төркеме.</kazontolgy:definition>
        <kazontolgy:question xml:lang="kg">Эмне кылды?эмне кылат?эмне кылып жатат? Эмне болду? Эмне болуп жатат? Кантет?</kazontolgy:question>
        <kazontolgy:question xml:lang="kz">не істеді?   не істейді? не қылған?</kazontolgy:question>
        <kazontolgy:question xml:lang="tatar">нишли? нәрсә эшли?</kazontolgy:question>
        <rdfs:label xml:lang="tr">Eylem</rdfs:label>
        <rdfs:label xml:lang="kz">Етістік</rdfs:label>
        <rdfs:label xml:lang="tatar">Фигыль</rdfs:label>
        <rdfs:label xml:lang="kg">Этиш</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#VA -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#VA">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#VP"/>
        <kazontolgy:definition xml:lang="tr">iyi çalışmak (хорошо работать), в) kitap okumak (читать книгу)</kazontolgy:definition>
        <kazontolgy:definition xml:lang="kz">Басыңқы бөлімі етістік болып келетін және ешқандай жалғаусыз байланысқан етістікті сөз тіркесі</kazontolgy:definition>
        <kazontolgy:definition xml:lang="tatar">Ияртүчесе фигыль белән бирелгән һәм бернинди дә ялгаулыклар булмаган фигыль сүзтезмәсе</kazontolgy:definition>
        <kazontolgy:example xml:lang="kz">жақсы оқиды, бүгін келдім, сылқ-сылқ күледі, оқығалы келіп отырмыз, бес ескерттім</kazontolgy:example>
        <kazontolgy:instance xml:lang="tatar">яхшы укыды, бүген килдем, елык-елык килә, укырга диеп килеп барабыз, биш исенә төшердем</kazontolgy:instance>
