    <!-- http://www.semanticweb.org/kazontolgy#ABL -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#ABL">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#Cases"/>
        <kazontolgy:instance xml:lang="kz">-дан, ден, -тан, -тен, -нан, -нен</kazontolgy:instance>
        <rdfs:label xml:lang="tatar">Чыгыш килеше кушымчалары</rdfs:label>
        <rdfs:label xml:lang="kz">Шығыс септік жалғаулары</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#ABL_C -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#ABL_C">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#CASES_category"/>
        <kazontolgy:definition xml:lang="tr">Çıkma, uzaklaşma, gelme ve girme durumunu gösterir</kazontolgy:definition>
        <kazontolgy:definition xml:lang="kz">Шығыс септік қимылдың, іс-әрекеттің неден, қайдан шыққанын, басталғанын білдіріп, сөйлемде толықтауыш және мекен, мезгіл пысықтауыш кызметін атқарады</kazontolgy:definition>
        <kazontolgy:question xml:lang="tr">Kimden? Nereden?</kazontolgy:question>
        <kazontolgy:question xml:lang="kg">Кимден? Эмнеден? Кайдан? Каяктан?</kazontolgy:question>
        <kazontolgy:question xml:lang="tatar">кемнән? нәрсәдән7</kazontolgy:question>
        <kazontolgy:question xml:lang="uz">кимдан?нимадан?</kazontolgy:question>
        <kazontolgy:question xml:lang="kz">кімнен? неден? қайдан?</kazontolgy:question>
        <rdfs:label xml:lang="tr">Ayrılma hali</rdfs:label>
        <rdfs:label xml:lang="kz">Шығыс септік</rdfs:label>
        <rdfs:label xml:lang="uz">чиқиш</rdfs:label>
        <rdfs:label xml:lang="kg">чыгыш</rdfs:label>
        <rdfs:label xml:lang="tatar">чыгыш килеше</rdfs:label>
    </owl:Class>
    


