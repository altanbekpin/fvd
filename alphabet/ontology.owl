        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#PSP"/>
        <rdfs:label xml:lang="kz">бастауыш</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kazontolgy#Pron -->

    <owl:Class rdf:about="http://www.semanticweb.org/kazontolgy#Pron">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kazontolgy#POS"/>
        <kazontolgy:definition xml:lang="tr">Adların yerini tutan sözcüklere adıl denir.</kazontolgy:definition>
        <kazontolgy:definition xml:lang="kg">Ат атооч сөздөр башка сөз түркүмдөрүнүн ордуна
 колдонулат. Булар заттарды, түшүнүктөрдү же заттардын кайсы
 бир белгилерин атабай туруп, аларды жалпылап көрсөтөт.</kazontolgy:definition>
        <kazontolgy:definition xml:lang="kz">есім сөздердің (зат есім, сын есім, сан есімнің) атын да, белгісін де, санын да атамай, тек солардың орнына қолданылады</kazontolgy:definition>
        <rdfs:label xml:lang="tr">Adıl</rdfs:label>
        <rdfs:label xml:lang="tatar">Алмашлык</rdfs:label>
        <rdfs:label xml:lang="kg">Ат атооч</rdfs:label>
        <rdfs:label xml:lang="kz">Есімдік</rdfs:label>
    </owl:Class>
    


