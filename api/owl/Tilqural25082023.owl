    <!-- http://www.semanticweb.org/til_qural_baitursyn#Пысықтауыш -->

    <owl:Class rdf:about="http://www.semanticweb.org/til_qural_baitursyn#Пысықтауыш">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/til_qural_baitursyn#Тұрлаусыз_мүше"/>
        <rdfs:subClassOf>
            <owl:Restriction>
